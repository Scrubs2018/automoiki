        padding-top 10px
    .b-container
        display table
        width 100%
        table-layout fixed
    &__left, &__middle, &__right
        display table-cell
        vertical-align top
    &__left
        text-align left
    &__left, &__middle, &__right
        @media $max_xs
            display block
            text-align center
    &__right
        text-align right
        // @media $max_xs
        //     width 0
        .b-header
            &__rside-row
                &:first-child
                    @media $max_xs
                        display none
    &__middle
        width 199px
        @media $max_xs
            width 150px
            margin 0 auto
            margin-top 25px
            margin-bottom 30px
    &__lside-row
        margin-top 40px
        @media $max_xs
            display none
    &__rside-row
        & + .b-header
            &__rside-row
                margin-top 44px
                @media $max_xs
                    margin-top 24px
    &__add-wash-wrap
        @media $max_xs
            display none
    &__top-menu-wrap, &__social-wrap
        @media $max_xs
            display none
    &__social-wrap, &__top-menu-wrap
        display inline-block
        vertical-align middle

.b-logo
