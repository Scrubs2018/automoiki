        font-size 19px
    &__inside
        margin-left -100px
        display inline-block
        vertical-align middle
        text-decoration none
        &:hover
            text-decoration underline
            color #ff4343
            opacity .71
        &:before
            content ''
            sprite($s-action)
            display block
            margin 0 auto

.b-top-menu
